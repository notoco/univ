<?xml version="1.0" encoding="UTF-8"?>
<window id="11112">
    <coordinates>
        <system>1</system>
        <posx>720</posx>
        <posy>420</posy>
    </coordinates>
    <controls>]

        <control type="group">
            <posx>0</posx>
            <posy>0</posy>
            <width>520</width>
        </control>
                <control type="image">
                    <width>520</width>
                    <height>140</height>
                    <texture>common/black-back.png</texture>
                </control>
        <control type="label" id="10071">
            <description>title</description>
            <posx>20</posx>
            <posy>20</posy>
            <width>480</width>
            <height>20</height>
            <label></label>
            <align>center</align>
            <font>font14</font>
        </control>
        <control type="list" id="10070">
            <description>button list</description>
            <posx>20</posx>
            <posy>60</posy>
            <width>480</width>
            <height>80</height>
            <onup>10070</onup>
            <ondown>10070</ondown>
            <itemlayout width="480">
                <control type="label">
                    <font>font13</font>
                    <align>center</align>
                    <aligny>center</aligny>
                    <label>$INFO[ListItem.Label]</label>
                </control>
            </itemlayout>
            <focusedlayout width="480">
                <control type="label">
                    <font>font13</font>
                    <align>center</align>
                    <aligny>center</aligny>
                    <label>[COLOR $VAR[ColorHighlight]]$INFO[ListItem.Label][/COLOR]</label>
                </control>
            </focusedlayout>
            <onfocus condition="String.IsEqual(Window(10000).Property(script.audio.profiles_autoclose),1)">PreviousMenu</onfocus>
            <animation effect="fade" end="100" delay="5000">Focus</animation>
        </control>
    </controls>
            <animation type="WindowOpen" reversible="false">
                <effect type="fade" start="0" end="100" time="400" />
                <effect type="slide" start="0,-181" end="0,0" time="400" />
            </animation>
            <animation type="WindowClose" reversible="false">
                <effect type="fade" start="100" end="0" time="400" />
                <effect type="slide" start="0,0" end="0,-181" time="400" />
            </animation>
</window>